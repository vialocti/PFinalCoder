<div>
    <h2>Productos en Lista</h2>
      <table class="table">
         <head>
          <tr>
            <th>id</th> 
            <th>Producto</th>
            <th>Decripcion</th>
            <th>Codigo</th>
            <th>Precio</th>
            <th>Stock</th>
            <th>Vistas productos</th>
          </tr>
         </head>
        <tbody>
        {{#each productos}}
            <tr>
             <td>{{this.id}}</td>
             <td> {{this.title}}</td>
             <td>{{this.description}}</td>
             <td>{{this.code}}</td>
             <td>{{this.price}}</td>
             <td>{{this.stock}}</td>
             <td>{{this.thumbnails}}</td>

            </tr>
        {{/each}}
        </tbody>
  </table>
</div>